obj-$(CONFIG_CPU_SUBTYPE_STX5197)	+= ilc3.o
obj-$(CONFIG_CPU_SUBTYPE_STX5206)	+= ilc3.o
obj-$(CONFIG_CPU_SUBTYPE_STX7100)	+= ilc3_stx7100.o
obj-$(CONFIG_CPU_SUBTYPE_STX7105)	+= ilc3.o
obj-$(CONFIG_CPU_SUBTYPE_STX7108)	+= ilc3.o
obj-$(CONFIG_CPU_SUBTYPE_STX7111)	+= ilc3.o
obj-$(CONFIG_CPU_SUBTYPE_STX7141)	+= ilc3.o
obj-$(CONFIG_CPU_SUBTYPE_STX7200)	+= ilc3.o
